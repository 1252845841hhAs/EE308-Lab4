@import"./styles/iconfont.wxss";

page,view,text,swiper,swiper-item,image,navitager{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
/* 主题颜色 */
page{
    --themecolor:#1296db;

}
.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 100%;
}
.background{
    box-sizing: border-box;
    height: 100%;
    width: 750rpx;
    position: absolute;
    z-index: -1;
    display: flex;
}
